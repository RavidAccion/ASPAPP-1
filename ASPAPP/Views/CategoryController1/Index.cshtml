@model IEnumerable<Category>
@{
    ViewData["Title"] = "Index";
}

<h1>Category</h1>

<table class="table table-bordered tabletable-striped" style="width:100%">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Order
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var obj in Model)
        {
            <tr>
                <td>
                    @obj.Name
                </td>
                <td>
                    @obj.DisplayOrder
                </td>
            </tr>
        }
     
    </tbody>
</table>
